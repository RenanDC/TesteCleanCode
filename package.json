{
  "name": "testecleancode",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --watch --colors --noStackTrace",
    "test:staged": "jest --passWithNoTests --findRelatedTests --silent",
    "test:ci": "jest --covarage --noStackTrace --silent"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RenanDC/TesteCleanCode.git"
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "npm run test:staged",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:ci"
    }
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RenanDC/TesteCleanCode/issues"
  },
  "homepage": "https://github.com/RenanDC/TesteCleanCode#readme",
  "devDependencies": {
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "standard": "^16.0.4"
  },
  "dependencies": {
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
